{
    "__base__": ["configs/det/models/FCOS/FCOS_resnet18.json", "tools/config/augmentation/base_augmentation.json"],
    "task": "detection",
    "num_gpus": 1,

    "optimizer":{
        "type": "SGD",
        "lr": 0.001,
        "momentum": 0.9,
        "weight_decay": 0.0001
    },

    "dataset": {
        "input_size": [640, 640],
        "type": "DetectionDataset",
        "train_data_path": "D:\\datasets\\huangluyao\\detection\\cai_qie_bu_liang\\train",
        "val_data_path": "D:\\datasets\\huangluyao\\detection\\cai_qie_bu_liang\\val"
    },

    "loader_cfg": {
        "batch_size": 4
    },

    "scheduler": {
        "warm_up_epochs": 5,
        "max_epochs": 100
    },
    "model": {
        "bbox_head":
            {
            "type": "FCOSHead",
            "num_classes": 2,
            "in_channels": 256,
            "feat_channels": 256,
            "stacked_convs": 2,
            "strides": [8,16,32, 64, 128],
            "regress_ranges":[[-1,64],[64,128],[128,256],[256,512],[512, 800]],
            "loss_bbox": {"type": "CIoULoss"}
            },
        "neck": {
            "type":"FPN",
            "in_channels": [64, 128, 256, 512],
            "out_channels": 256,
            "num_outs": 5,
            "start_level": 1,
            "end_level": -1,
            "add_extra_convs": true,
            "relu_before_extra_convs": true
        }
    },
    "pretrained": "export/detection/FCOS_ResNet_FPN_VOCDataset/2021-07-02-18-02-32/checkpoints/val_best.pth"
}
