{
    "gpu_id": 0,
    "optimizer": {
        "type": "AdamW",
        "lr": 0.001,
        "weight_decay": 0.0001
    },
    "dataset": {
        "input_size": [256, 256],
        "type": "SegDataset",
        "use_mosaic": false,
        "train_data_path": "D:\\data\\seg\\dos_stc/train",
        "val_data_path": "D:\\data\\seg\\dos_stc/val"
    },
    "loader_cfg": {
        "batch_size": 4,
        "num_workers": 4
    },

    "use_ema":false,
    "max_epochs": 1,
    "max_iters": null,

    "hook_cfgs": [
        {
            "type": "CosineRestartLrUpdaterHook",
            "min_lr": 1e-5,
            "by_epoch": true,
            "warmup": "linear",
            "warmup_iters": 50,
            "warmup_ratio": 0.1,
            "periods": [25, 75, 125, 200],
            "restart_weights": [1, 0.75, 0.5, 0.5]
        },
        {
            "type": "OptimizerHook",
            "grad_clip": null
        },
        {
            "type": "SegEvalHook",
            "start": null,
            "evaluation_first": false,
            "interval": 1
        },
        {
            "type": "IterTimerHook"
        },
        {
            "type": "ExportOnnxHook",
            "input_name":"input_image",
            "output_name":"output_names"
        }
    ]
}
