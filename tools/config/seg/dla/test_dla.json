{
    "__base__": [
        "configs/seg/models/dla/dla_resnet18s8.json",
        "tools/config/__base__/augmentation/base_augmentation.json",
        "tools/config/__base__/seg_base.json"
    ],
    "task": "segmentation",
    "loader_cfg": {
        "batch_size": 8,
        "num_workers": 0
    },

    "hook_cfgs": [
        {
            "type": "CosineRestartLrUpdaterHook",
            "min_lr": 1e-5,
            "by_epoch": true,
            "warmup": "linear",
            "warmup_iters": 50,
            "warmup_ratio": 0.1,
            "periods": [25, 75, 125, 200],
            "restart_weights": [1, 0.75, 0.5, 0.5]
        },
        {
            "type": "OptimizerHook",
            "grad_clip": null
        },
        {
            "type": "SegEvalHook",
            "start": null,
            "evaluation_first": false,
            "interval": 1
        },
        {
            "type": "IterTimerHook"
        },
        {
            "type": "SSLWithGANHook",
            "remain_image_folder": "/workspace/data/seg/df_new/train/images",
            "loader_cfg": "auto",
            "priority": "VERY_HIGH"
        }
    ]
}
