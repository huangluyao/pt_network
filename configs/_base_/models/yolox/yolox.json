{
    "model": {
        "type": "YOLOX",
        "backbone": {
            "out_levels": [
                3,
                4,
                5
            ],
            "in_channels": 3
        },

        "bbox_head": {
            "type": "YOLOXHead",
            "num_classes": 1,
            "in_channels": 128,
            "feat_channels": 128,
            "stacked_convs": 2,
            "strides": [8,16,32],
            "loss_cls": {"type": "FocalLoss", "gamma": 2.0, "alpha": 0.25, "loss_weight": 1.0},
            "loss_obj": {"type": "FocalLoss", "gamma": 2.0, "alpha": 0.25, "loss_weight": 1.0},
            "loss_bbox": {"type": "IoULoss", "loss_weight": 1.0}
        }
    },

    "train_cfg":
    {
        "score_thr":0.01,
        "nms":  0.5,
        "max_per_img": 100
    },
    "test_cfg":
    {}
}