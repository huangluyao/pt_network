{
    "_base_": "../../../_base_/models/fcos/fcos.json",
    "pretrained": "model_zoo/pth/backbone/smart/smart.pth",
    "model": {
        "backbone": {
            "type": "SmartBackbone",
            "depth_multiple": 0.33,
            "width_multiple": 0.50,
            "num_classes": null,
            "act_cfg": {"type": "SiLU"},
            "norm_cfg":{"type":"BN", "requires_grad":true},
            "out_levels": [3, 4, 5 ],
            "use_transformer": false
        },
        "neck": {
            "type": "CSP_PAN",
            "input_channels": [128, 256, 512],
            "output_channels": [128, 128, 128],
            "strides": [8, 16, 32],
            "act_cfg": {"type": "SiLU"},
            "norm_cfg":{"type":"BN", "requires_grad":true}
        },
        "bbox_head": {
            "type": "FCOSHead",
            "num_classes": 1,
            "in_channels": 128,
            "feat_channels": 128,
            "stacked_convs": 2,
            "strides": [8,16,32],
            "regress_ranges":[[-1,64],[64,128],[128,256]],
            "loss_bbox": {"type": "CIoULoss"},
            "loss_cls":{"type":"FocalLoss", "gamma":2, "alpha": 0.25, "loss_weight": 1.0},
            "loss_centerness":{"type":"CrossEntropyLoss", "use_sigmoid":true, "loss_weight": 1.0}
        },

        "train_cfg":
        {
            "score_thr":0.3,
            "nms":  0.3,
            "max_per_img": 100
        },
        "test_cfg":
        {
            "score_thr":0.3
        }

    }
}
