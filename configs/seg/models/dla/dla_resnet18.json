{
    "pretrained": "model_zoo/pth/backbone/resnet/resnet18.pth",
    "model": {
        "type": "EncoderDecoder",
        "backbone": {
            "type": "ResNet",
            "depth": 18,
            "out_levels": [1, 2, 3, 4, 5],
            "num_classes": null
        },
        "decode_head": {
            "type": "DLAHead",
            "in_channels": [64, 64, 128, 256, 512],
