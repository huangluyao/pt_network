{
    "pretrained": "model_zoo/pth/backbone/smart/smart.pth",
    "model": {
        "type": "EncoderDecoder",
        "backbone": {
            "type": "SmartBackbone",
            "depth_multiple": 0.33,
            "width_multiple": 0.50,
            "num_classes": null,
            "act_cfg": {"type": "SiLU"},
            "norm_cfg":{"type":"BN", "requires_grad":false},
            "in_channels": 3,
            "out_levels": [1, 2, 3, 4, 5]
        },
        "decode_head": {
            "type": "DLAHead",
            "in_channels": [32, 64, 128, 256, 512],
            "num_classes": 1,
            "align_corners": false,
            "dropout": 0.0,
            "upsample_cfg": {"type": "bilinear", "scale_factor":2, "align_corners": false},
            "norm_cfg":{"type": "BRN", "rmax_inc_step": 1000, "dmax_inc_step": 500},
            "act_cfg": {"type": "xUnitS"},
            "loss": [{"type": "LovaszLoss"}, {"type": "CrossEntropyLoss"}, {"type": "BoundaryLoss"}]
        }
    }
}
