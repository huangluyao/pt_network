{
    "pretrained": "model_zoo/pth/backbone/resnet/ResNet50_v1c.pth",
    "model": {
        "type": "EncoderDecoder",
        "backbone": {
            "type": "ResNet",
            "depth": 50,
            "out_levels": [0, 1, 2, 3, 4, 5],
            "strides": [1, 2, 1, 1],
            "dilations": [1, 1, 2, 4],
            "num_classes": null,
            "contract_dilation": true,
            "deep_stem": true
        },
        "decode_head": {
            "type": "DAHead",
            "pam_channels": 64,
            "in_channels": 2048,
            "in_index": 5,
            "head_width": 512,
            "final_drop": 0.1,
            "num_classes": 21,
            "norm_cfg": {"type": "BN", "requires_grad": true},
            "align_corners": false,
            "loss_decode": {"type": "CrossEntropyLoss", "use_sigmoid": false, "loss_weight": 1.0}
        },
        "auxiliary_head": {
            "type": "FCNHead",
            "in_channels": 1024,
            "in_index": 4,
            "head_width": 256,
            "num_convs": 1,
            "concat_input": false,
            "final_drop": 0.1,
            "num_classes": 21,
            "norm_cfg": {"type": "BN", "requires_grad": true},
            "align_corners":false,
            "loss_decode": {"type": "CrossEntropyLoss", "use_sigmoid": false, "loss_weight": 0.4}
        }
    }
}
