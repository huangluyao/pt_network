{
  "model": {
    "type": "TwoStageInpaintor",
    "disc_input_with_mask": true,
    "encdec":
    {
      "type": "DeepFillEncoderDecoder",
      "stage1": {
        "type": "GLEncoderDecoder",
        "encoder": {
          "type": "DeepFillEncoder",
          "conv_type": "gated_conv",
          "channel_factor": 0.75,
          "padding_mode": "reflect"
        },
        "decoder": {
          "type": "DeepFillDecoder",
          "conv_type": "gated_conv",
          "in_channels": 96,
          "channel_factor": 0.75,
          "out_act_cfg": {
            "type": "Tanh"
          },
          "padding_mode": "reflect"
        },
        "dilation_neck": {
          "type": "GLDilationNeck",
          "in_channels": 96,
          "conv_type": "gated_conv",
          "act_cfg": {
            "type": "ELU"
          },
          "padding_mode": "reflect"
        }
      },
      "stage2": {
        "type": "DeepFillRefiner",
        "encoder_attention": {
          "type": "DeepFillEncoder",
          "encoder_type": "stage2_attention",
          "conv_type": "gated_conv",
          "channel_factor": 0.75,
          "padding_mode": "reflect"
        },
        "encoder_conv": {
          "type": "DeepFillEncoder",
          "encoder_type": "stage2_conv",
          "conv_type": "gated_conv",
          "channel_factor": 0.75,
          "padding_mode": "reflect"
        },
        "dilation_neck": {
          "type": "GLDilationNeck",
          "in_channels": 96,
          "conv_type": "gated_conv",
          "act_cfg": {
            "type": "ELU"
          },
          "padding_mode": "reflect"
        },
        "contextual_attention": {
          "type": "ContextualAttentionNeck",
          "in_channels": 96,
          "conv_type": "gated_conv",
          "padding_mode": "reflect"
        },
        "decoder": {
          "type": "DeepFillDecoder",
          "in_channels": 192,
          "conv_type": "gated_conv",
          "out_act_cfg": {
            "type": "Tanh"
          },
          "padding_mode": "reflect"
        }
      }
    },
    "disc": 
    {
      "type": "MultiLayerDiscriminator",
      "in_channels": 4,
      "max_channels": 256,
      "fc_in_channels": null,
      "num_convs": 6,
      "norm_cfg": null,
      "act_cfg": {"type": "LeakyReLU", "negative_slope": 0.2},
      "out_act_cfg": {"type": "LeakyReLU", "negative_slope": 0.2},
      "with_spectral_norm": true
    },
    "loss_gan": {"type": "WGANLossNS", "loss_weight": 0.1},
    "loss_l1_hole": {"type": "L1Loss", "loss_weight": 1.0},
    "loss_l1_valid": {"type": "L1Loss", "loss_weight": 1.0}
  }
}